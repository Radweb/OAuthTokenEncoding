{
    "name": "radweb/oauth-token-encoding",
    "description": "Alternate Encoding for OAuth 2 Token Responses",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Harper",
            "email": "hello@dans.email"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4",
        "zendframework/zend-diactoros": "^1.1",
        "symfony/http-foundation": "~2.3|~3.0",
        "league/oauth2-server": "^4.1"
    },
    "suggest": {
        "zendframework/zend-diactoros": "Required to use PSR-7 adaptor (^1.1)",
        "symfony/http-foundation": "Required to use Symfony HTTP Foundation Adaptor (~2.3|~3.0)",
        "league/oauth2-server": "Required to use the Laravel middleware (^4.0)"
    },
    "autoload": {
        "psr-4": {
            "Radweb\\OAuthTokenEncoding\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radweb\\OAuthTokenEncoding\\Tests\\": "tests"
        }
    }
}
